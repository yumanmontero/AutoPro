@using AutoPro.Models
@model ReporteEmpleadosViewModels
@{
    ViewBag.Title = "Reporte de Empleados";
    UsuarioViewModel user = new UsuarioViewModel();
    if (HttpContext.Current.Session["User"] != null)
    {
        user = HttpContext.Current.Session["User"] as UsuarioViewModel;
    }
    else
    {

        user = HttpContext.Current.Session["User"] as UsuarioViewModel;

    }
    
}

<section class="content-header">
    <h1 id="titulo_preventa">
        @ViewBag.Title

        <small>@user.Concesionario</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="/Home/"><i class="fa fa-dashboard"></i> Inicio</a></li>
        <li><a href="/Administración/">Administración</a></li>
        <li class="active">@ViewBag.Title</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">

    <div class="row">

        <div class="col-lg-4 col-xs-4">
            <!-- Date and time range -->
            <div class="form-group">
                <label>Filtro por Periodos</label>
                <div class="input-group">
                    <button type="button" class="btn btn-default" id="daterange-btn">
                        <span>
                            <i class="fa fa-calendar"></i> Rango de Fecha
                        </span>
                        <i class="fa fa-caret-down"></i>
                    </button>
                    <button type="button" href="javascript:{}" id="boton_rango_fecha" class="btn btn-default">
                        <i class="fa fa-search"></i>
                    </button>
                </div>
            </div>
            <!-- /.form group -->
        </div>

        <div class="col-lg-4 col-xs-4">
            <!-- Date and time range -->
            <div class="form-group">
                <label>Rango Consultados</label>
                <div class="input-group">
                    <h3>@Model.Rango_de_Consulta</h3>
                </div>
            </div>
            <!-- /.form group -->
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12 col-md-12 col-xs-12">
            <div class="box-body">
                <div class="callout callout-info">
                    <h4>Vendedores</h4>
                </div>
            </div>
        </div>
    </div>


    <div class="row">
        <div class="row">
            <div class="col-lg-2 col-md-2 col-xs-2">

            </div>
            <div class="col-lg-4 col-md-4 col-xs-4">
                <div class="small-box bg-aqua">
                    <div class="inner">
                        <h3>@Model.Vendedor_Venta_Total</h3>

                        <p>Ventas</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa-cubes"></i>
                    </div>
                    <a href="javascript:{}" class="small-box-footer">
                        Totales <i class="fa fa-info"></i>
                    </a>
                </div>
            </div>

            <div class="col-lg-4 col-md-4 col-xs-4">
                <div class="small-box bg-green">
                    <div class="inner">
                        <h3>@Model.Vendedor_Comision_Total</h3>

                        <p>Comisiones</p>
                    </div>
                    <div class="icon">
                        <i class="fa fa-money"></i>
                    </div>
                    <a href="javascript:{}" class="small-box-footer">
                        Totales <i class="fa fa-info"></i>
                    </a>
                </div>
            </div>

            <div class="col-lg-2 col-md-2 col-xs-2">

            </div>


        </div>

        <div class="row">
            <div class="col-lg-8 col-md-8 col-xs-4">

            </div>

        </div>
        

        <div class="col-md-3">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">Vendedores</h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                    </div>
                </div>
                <!-- /.box-header -->
                <div class="box-body no-padding">
                    <table class="table table-striped">
                        <tr>
                            <td>Ventas totales</td>
                            <td>101</td>
                        </tr>
                        <tr>
                            <td>Comisiones totales</td>
                            <td>$21,400</td>
                        </tr>
                        <tr>
                            <td>Ventas por el Manager</td>
                            <td>2</td>
                        </tr>
                    </table>
                </div>
                <!-- /.box-body -->
            </div>

            <!-- DONUT CHART -->
            <div class="box box-danger">
                <div class="box-header with-border">
                    <h3 class="box-title">Donut Chart</h3>

                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-minus"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body chart-responsive">
                    <div class="chart" id="sales-chart" style="height: 200px; position: relative;"></div>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->

        </div>
        <!-- /.col (LEFT) -->
        <div class="col-md-6">
            <!-- LINE CHART -->
            <div class="box">
                <div class="box-header">
